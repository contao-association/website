        <body>
            <trans-unit id="tl_member.tax_id.0">
                <source>Tax ID</source>
                <target>Steuernummer</target>
            </trans-unit>
            <trans-unit id="tl_member.tax_id.1">
                <source>Enter a tax ID for this company.</source>
                <target>Geben Sie die Steuernummer der Firma ein.</target>
            </trans-unit>
            <trans-unit id="tl_member.membership.0">
                <source>Subscription</source>
                <target>Abonnement</target>
            </trans-unit>
            <trans-unit id="tl_member.membership.1">
                <source>Please select a subscription level.</source>
                <target>Wähle ein Abonnement.</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_member.0">
                <source>Association membership</source>
                <target>Vereinsmitgliedschaft</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_member.1">
                <source>Become an active member of the Contao Association</source>
                <target>Aktivmitglied der Contao Association werden</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_interval.0">
                <source>Billing</source>
                <target>Abrechnung</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_interval.1">
                <source>Select if the subscription is paid monthly or yearly (if applicable)</source>
                <target>Wähle monatliche oder jährliche Abrechnung (falls zutreffend).</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_interval.month">
                <source>monthly</source>
                <target>monatlich</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_interval.year">
                <source>yearly</source>
                <target>jährlich</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_amount.0">
                <source>Voluntary donation</source>
                <target>Freiwillige Spende</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_amount.1">
                <source>Enter an optional donation for each invoice period.</source>
                <target>Gib eine optionale Spende pro Rechnungsperiode ein.</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_start.0">
                <source>Subscription start date</source>
                <target>Startdatum Abonnement</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_start.1">
                <source>Enter a start date for the subscription.</source>
                <target>Gib ein Startdatum für das Abonnement ein.</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_stop.0">
                <source>Subscription stop date</source>
                <target>Stopdatum Abonnement</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_stop.1">
                <source>Enter a stop date for the subscription.</source>
                <target>Gib ein Stopdatum für das Abonnement ein.</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_invoiced.0">
                <source>Subscription invoiced until</source>
                <target>Abonnement verrechnet bis</target>
            </trans-unit>
            <trans-unit id="tl_member.membership_invoiced.1">
                <source>The subscription has been invoiced until this date.</source>
                <target>Das Abonnement wurde bis zu diesem Datum verrechnet.</target>
            </trans-unit>

            <trans-unit id="tl_member.listing.0">
                <source>Public display</source>
                <target>Öffentliche Anzeige</target>
            </trans-unit>
            <trans-unit id="tl_member.listing.1">
                <source>I want to be publicly displayed (e.g. on contao.org)</source>
                <target>Ich möchte als Supporter/Sponsor genannt werden (z.B. auf contao.org).</target>
            </trans-unit>
            <trans-unit id="tl_member.listing_name.0">
                <source>Listing name</source>
                <target>Anzeigename</target>
            </trans-unit>
            <trans-unit id="tl_member.listing_name.1">
                <source>Enter a custom name for the listing.</source>
                <target>Gib einen eigenen Namen für die Auflistung ein.</target>
            </trans-unit>
            <trans-unit id="tl_member.listing_link.0">
                <source>Link</source>
                <target>Link</target>
            </trans-unit>
            <trans-unit id="tl_member.listing_link.1">
                <source>Enter a custom link for the listing.</source>
                <target>Gibt einen eigenen Link für die Auflistung ein.</target>
            </trans-unit>
            <trans-unit id="tl_member.listing_logo.0">
                <source>Sponsor Logo</source>
                <target>Sponsor-Logo</target>
            </trans-unit>
            <trans-unit id="tl_member.listing_link.1">
                <source>Enter a path to the sponsor logo.</source>
                <target>Gib den Pfad zu einer Logo-Datei ein.</target>
            </trans-unit>

            <trans-unit id="tl_member.member_log_note.0">
                <source>Note</source>
                <target>Notiz</target>
            </trans-unit>
            <trans-unit id="tl_member.member_log_note.1">
                <source>Please enter the note.</source>
                <target>Bitte geben Sie eine Notiz ein.</target>
            </trans-unit>
            <trans-unit id="tl_member.log.0">
                <source>View logs</source>
                <target>Historie &amp; Notizen ansehen</target>
            </trans-unit>
            <trans-unit id="tl_member.log.1">
                <source>View logs of member ID %s</source>
                <target>Historie &amp; Notizen für Mitglied ID %s ansehen</target>
            </trans-unit>
            <trans-unit id="tl_member.log_legend">
                <source>Log settings</source>
                <target>Mitglieder-Log</target>
            </trans-unit>
            <trans-unit id="tl_member.subscription_legend">
                <source>Abonnement</source>
            </trans-unit>
            <trans-unit id="tl_member.listing_legend">
                <source>Listing</source>
                <target>Auflistung</target>
            </trans-unit>
        </body>
